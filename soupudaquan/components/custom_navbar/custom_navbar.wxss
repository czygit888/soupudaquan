.nav_container{
    position: fixed;
    z-index: 100;
    top:0;
    left:0;
    width:100%;
}
.nav_container>view:last-child{
    position: absolute;
}
.menubutton{
    border:1rpx solid rgba(0,0,0,0.04);
    box-sizing: border-box;
    padding: 12rpx 18rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-radius: 50rpx;
    position: absolute;
}
.menubutton>view{
    width:33%;
    height:100%;
    display: flex;
    align-items: center;
    justify-content: center;
}
.menubutton image{
    width: 100%;
    height:100%;
    vertical-align: middle;
}